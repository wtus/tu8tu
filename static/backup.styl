@import "default.styl"
/*************************样式**************************/
.d-inline-block
  display inline-block

.fright
  float right

.flex
  display flex

.flex-1
  flex 1

.fjc-start
  justify-content flex-start

.fjc-end
  justify-content flex-end

.fjc-center
  justify-content flex-end

.fjc-around
  justify-content space-around

.fjc-between
  justify-content space-between

.fai-start
  align-items flex-start

.fai-end
  align-items flex-end

.fai-center
  align-items center

.fai-baseline
  align-items baseline

.fai-stretch
  align-items stretch

.fas-start
  align-self flex-start

.fas-end
  align-self flex-end

.fas-center
  align-self center

.fas-stretch
  align-self stretch

/*flex布局居中*/
.flex-center
  display flex
  align-items center
  justify-content center

.flex-column
  display flex
  flex-direction column

/*显示省略号*/
.ellipsis
  white-space nowrap
  overflow hidden
  text-overflow ellipsis

/**************************函数/mixin*************************/
wh(w = auto, h = auto)
  width w
  height h

/**************************动画*************************/
@keyframes fade-in {
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }
}

@-webkit-keyframes fade-out {
  0% {
    opacity: 1
  }
  100% {
    opacity: 0
  }
}

